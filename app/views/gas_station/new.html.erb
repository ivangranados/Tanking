<h1>Crear una GasStation</h1>
<p><%= flash[:notice] %> </p>
<p><%= link_to "Regresar", tanking_log_index_path(:id => params[:id]) %></p>


<%= form_for(:gas_station, :url => gas_station_create_path(:id => params[:id])) do |c| %>
  <table>
    <tr>
        <th>Nombre</th>
        <td><%= c.text_field (:name) %></td>
    </tr>
  </table>

  <%= submit_tag("Crear Nuevo Estacion") %>

<h3>Estaciones</h3>
<table class="table table-hover" width = 15% >
	<thead>
		<tr>
			<th>Nombre</th>
		</tr>
	</thead>
	<tbody>
	<% @show.each do |stations| %>
  		<tr>
    		<td><%= stations.name %></td>
  		</tr>
	<% end %>
	</tbody>
</table>
<% end %>
